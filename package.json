{
  "name": "atom-ftp-editor",
  "main": "./lib/main",
  "version": "0.5.0",
  "description": "With this package every file that you edit is automatically upload to the server via ftp and you can download the entire project to your local computer",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "ftp-editor:create",
      "ftp-editor:download",
      "ftp-editor:single-download",
      "ftp-editor:current-download",
      "ftp-editor:current-upload"
    ]
  },
  "repository": "https://github.com/amitmtrn/atom-ftp-editor",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "ftp": "^0.3.10",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel-plugin-syntax-jsx": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.0",
    "node-promises": "^0.2.1"
  }
}
